{
    "name": "qwick",
    "version": "0.1.62",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "rm -r dist && tsc -p tsconfig.build.json && cp ./src/qwick/index.css ./dist",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "deploy": "npm run build && npm publish",
        "test": "vitest"
    },
    "dependencies": {
        "@peulicke/algorithms": "^1.0.7",
        "@peulicke/geometry": "^1.0.17",
        "@peulicke/image": "^1.0.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "three": "^0.177.0",
        "vitest": "^3.2.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.6",
        "@types/three": "^0.177.0",
        "@vitejs/plugin-react": "^4.5.1",
        "eslint": "^9.28.0",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.2.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.33.1",
        "vite": "^6.3.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Peulicke/qwick.git"
    },
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*.js",
        "./graphics/*": "./dist/graphics/*.js",
        "./button/*": "./dist/button/*.js"
    },
    "files": [
        "dist/"
    ]
}
