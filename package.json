{
    "name": "qwick",
    "version": "0.1.101",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "rm -r dist; tsc -p tsconfig.build.json && cp ./src/qwick/index.css ./dist",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "deploy": "npm run build && npm publish",
        "test": "vitest"
    },
    "dependencies": {
        "@peulicke/algorithms": "^1.0.11",
        "@peulicke/geometry": "^1.0.33",
        "@peulicke/image": "^1.0.1",
        "@peulicke/mesh": "^1.0.1",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "three": "^0.182.0",
        "vitest": "^4.0.16"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/three": "^0.182.0",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.39.2",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "globals": "^16.5.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.50.1",
        "vite": "^7.3.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Peulicke/qwick.git"
    },
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*.js",
        "./graphics/*": "./dist/graphics/*.js",
        "./button/*": "./dist/button/*.js"
    },
    "files": [
        "dist/"
    ]
}
